{

  "manifest_version": 2,
  "name": "ButWhyMdl",
  "version": "1.0",

  "description": "But why modals? (removes modals)",

  "icons": {
    "48": "icons/icon-48.png"
  },

  "browser_action": {
    "default_icon": "icons/icon-48.png",
    "default_title": "But Why Modal?",
    "default_popup": "popup/menu.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["butWhyMdl.js"]
    }
  ]

}
